<div
  class="tab-pane fade text-white"
  id="nav-seasonal-scoreboard"
  role="tabpanel"
  aria-labelledby="nav-seasonal-scoreboard-tab"
>
  <div id="seasonal-scoreboard" class="table-responsive">
    <table class="table table-dark">
      <thead>
        <tr>
          <th scope="col">Rank</th>
          <th scope="col">Name</th>
          <th scope="col">Displayed Badges</th>
          <th scope="col">Total Score</th>
        </tr>
      </thead>
      <tbody>
        <% if (locals.seasonalResults) { %>
          <% for(let i = 0; i < locals.seasonalResults.length; i++) { %>
            <tr>
              <th scope="row"><%= i + 1 %></th>
              <td><%= locals.seasonalResults[i].display_name %></td>
              <td>Add Badges...</td>
              <td><%= locals.seasonalResults[i].points %></td>
            </tr>
          <% } %>
        <% } else { %>
          <tr>
            <td colspan="4">No results found.</td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>
</div>
